<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <include android:id="@id/toolbar_cancel_done_header" android:layout_width="fill_parent" android:layout_height="wrap_content" layout="@layout/toolbar_cancel_done_header" />
    <ScrollView android:id="@id/create_channel_info_container" android:background="@color/white" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
            <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <TextView android:textSize="@dimen/channel_owner_screen_text_view_size" android:id="@id/channel_owner_intro" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginTop="@dimen/channel_owner_screen_margin_top_bottom" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:layout_marginBottom="@dimen/channel_owner_screen_margin_top_bottom" android:text="@string/channel_owner_profile_warning" />
                <TextView android:textSize="@dimen/channel_owner_screen_text_view_size" android:layout_gravity="right" android:id="@id/channel_owner_required_field" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:text="@string/channel_owner_profile_required_field" />
                <include android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:layout_marginBottom="@dimen/channel_owner_screen_margin_top_bottom" layout="@layout/divider_common" />
            </LinearLayout>
            <TextView android:textSize="@dimen/channel_owner_screen_text_view_size" android:textColor="@color/black" android:id="@id/channel_owner_full_name" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginTop="@dimen/new_channel_activity_channel_name_margin_top" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:text="@string/channel_owner_profile_full_name" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner_user_name" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
            <TextView android:textSize="@dimen/channel_owner_screen_text_view_size" android:textColor="@color/black" android:id="@id/channel_owner_contact" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginTop="@dimen/new_channel_activity_channel_name_margin_top" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:text="@string/channel_owner_profile_contact_information" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner_company_name" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_contact_information_hint" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
            <TextView android:textSize="@dimen/channel_owner_screen_text_view_size" android:textColor="@color/black" android:id="@id/channel_owner__email" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginTop="@dimen/new_channel_activity_channel_name_margin_top" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:text="@string/channel_owner_profile_email" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__secondary_email" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_email_hint_second_email" android:singleLine="true" android:inputType="textPersonName" style="@style/BB10Edit" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__phone_number" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_email_hint_phone_numer" android:singleLine="true" android:inputType="phone|textShortMessage" style="@style/BB10Edit" />
            <TextView android:textSize="@dimen/channel_owner_screen_text_view_size" android:textColor="@color/black" android:id="@id/channel_owner_address" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginTop="@dimen/new_channel_activity_channel_name_margin_top" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:text="@string/channel_owner_profile_address" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__street" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_address_hint_street_address" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__city" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_address_hint_city" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__state" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_address_hint_state" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__zip" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_address_hint_zip" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
            <EditText android:textSize="@dimen/channel_owner_screen_edit_text_size" android:id="@id/channel_owner__country" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/channel_owner_screen_margin_left_right" android:layout_marginRight="@dimen/channel_owner_screen_margin_left_right" android:hint="@string/channel_owner_profile_address_hint_country" android:singleLine="true" android:inputType="textShortMessage" style="@style/BB10Edit" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>